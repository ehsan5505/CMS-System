<% @page_title = "Edit #{@admin.name}" %>

  <%= link_to("<< Back to Index",{:action=>'index'},:class=>"btn btn-default btn-xs pull-right") %>
    <h2> Editing Admin </h2>
	<%= form_for(:admin,:url=>{:action=>'update',:id=>@admin.id},:class=>"form-horizontal") do |f| %>
	  
