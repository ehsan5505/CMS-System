<%= error_messages_for(@admin) %>	
	<table>
	    <tr class="form-group">
	      <th><%= f.label("First-name:",:for=>:first_name,:class=>"col-sm-2 control-label") %></th>
	      <td class="col-sm-10"><%= f.text_field(:first_name,:class=>"form-control",:placeholder=>"First Name" ) %></td>
	    </tr>
	    <tr class="form-group">
	      <th><%= f.label("Last-name:",:for=>:last_name,:class=>"col-sm-2 control-label") %></th>
	      <td class="col-sm-10"><%= f.text_field(:last_name,:class=>"form-control",:placeholder=>"Last Name") %></td>
	    </tr>
	    <tr class="form-group">
	      <th><%= f.label(:email,:for=>:email,:class=>"control-label col-sm-2") %></th>
	      <td class="col-sm-10"><%= f.text_field(:email,:class=>"form-control",:placeholder=>"Email Address") %></td>
	    </tr>
	    <tr class="form-group">
	      <th><%= f.label(:username,:for=>:username,:class=>"control-label col-sm-2") %></th>
	      <td class="col-sm-10"><%= f.text_field(:username,:class=>"form-control",:placeholder=>"Username") %></td>
	    </div>
	    </tr>
	    <tr>
	    <div class="form-group">
	      <th><%= f.label(:password,:for=>:password,:class=>"control-label col-sm-2") %></th>
	      <td class="col-sm-10"><%= f.password_field(:password,:class=>"form-control",:placeholder=>"Password") %></td>
	    </div>
	    </tr>
	    <tr>
	    <div class="form-group">
	      <th><%= f.label("Password",:for=>:password_confirmation,:class=>"control-label col-sm-2") %></th>
	      <td class="col-sm-10"><%= f.password_field(:password_confirmation,:class=>"form-control",:placeholder=>"Repeat Password") %></td>
	    </div>
	    </tr>
	</table>
	    